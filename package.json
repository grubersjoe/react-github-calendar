{
  "name": "react-github-calendar",
  "version": "4.0.6",
  "description": " A flexible React component to display a GitHub contributions graph",
  "author": "Jonathan Gruber <gruberjonathan@gmail.com>",
  "license": "MIT",
  "homepage": "https://grubersjoe.github.io/react-github-calendar/",
  "repository": "grubersjoe/react-github-calendar",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "deploy": "gh-pages -d example/build",
    "dev": "rollup -c -w",
    "format": "prettier --write './*' 'src/**/*' 'example/**/*'",
    "lint": "eslint 'src/**/*' 'example/src/**/*'",
    "predeploy": "cd example && ./sync-version.js && pnpm install && npm run build",
    "prepare": "npm run build",
    "prepublishOnly": "npx tsc && npm run lint && npm run build"
  },
  "dependencies": {
    "react-activity-calendar": "^2.1.8"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/tinycolor2": "^1.4.6",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.44.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "gh-pages": "^6.0.0",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^4.4.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "rollup-plugin-postcss": "^4.0.2",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "files": [
    "build"
  ],
  "browserslist": [
    "last 2 chrome version",
    "last 2 firefox version",
    "last 2 safari version"
  ]
}
